-- Fake Notification
local function createNotification(username, petName, weight, age)
	local notif = Instance.new("TextLabel")
	notif.Size = UDim2.new(1, 0, 0, 24)
	notif.Position = UDim2.new(0, 0, 0, -24)
	notif.BackgroundTransparency = 1
	notif.TextColor3 = Color3.fromRGB(255, 255, 255)
	notif.Font = Enum.Font.GothamSemibold
	notif.TextSize = 14
	notif.Text = username .. " spawned a " .. petName .. " (" .. weight .. "kg, Age " .. age .. ")"
	notif.Parent = ScreenGui

	local tweenIn = TweenService:Create(notif, TweenInfo.new(0.5), { Position = UDim2.new(0, 0, 0, 20) })
	local tweenOut = TweenService:Create(notif, TweenInfo.new(0.5), { Position = UDim2.new(0, 0, 0, -24) })

	tweenIn:Play()
	tweenIn.Completed:Wait(2)
	tweenOut:Play()
	game.Debris:AddItem(notif, 3)
end

-- Username generator
local usernames = { "ShinDev", "PixelRancher", "PetMaster99", "BotanicBoy", "GardeniaQueen", "LuaLord", "RaccoonRaiser", "EggGuru" }

local function getRandomUsername()
	return usernames[math.random(1, #usernames)]
end

-- Spawn Button Logic
SpawnButton.MouseButton1Click:Connect(function()
	local petName = PetNameInput.Text ~= "" and PetNameInput.Text or "Unknown Pet"
	local weight = tonumber(KgInput.Text) or 1
	local age = tonumber(AgeInput.Text) or 1
	local username = getRandomUsername()

	Spawner.SpawnPet(petName, weight, age)
	createNotification(username, petName, weight, age)
end)
